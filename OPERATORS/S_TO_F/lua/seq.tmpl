return require 'Q/UTILS/lua/code_gen' {
   declaration = [[
#include "q_incs.h"
#include "seq_struct.h"
extern int
seq_${out_qtype}(
  ${out_ctype} *X,
  uint64_t nX,
  ${args_ctype} *ptr_in,
  uint64_t idx // which element of vector 
  );
   ]],

   definition = [[

#include "_seq_${out_qtype}.h"
int
seq_${out_qtype}(
  ${out_ctype} *X,
  uint64_t nX,
  ${args_ctype} *ptr_in,
  uint64_t idx // which element of vector 
  )
{
  int status = 0;
  ${out_ctype} start = ptr_in->start;
  ${out_ctype} by = ptr_in->by;
  start += (idx *by); // offset based on index
  ${out_ctype} j = start;
  for ( uint64_t i = 0; i < nX; i++ ) { 
    X[i] = (${out_ctype}) j;
    j += by;
  }
  return status;
}
   ]]
}
