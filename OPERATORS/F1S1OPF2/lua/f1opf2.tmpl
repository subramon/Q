return require 'Q/UTILS/lua/code_gen' {
   declaration = [[
#include <math.h>
#include <stdio.h>
#include <stdbool.h>
#include <stdint.h>
#include <stdlib.h>
#include <unistd.h>
#include "q_macros.h"
//START_FOR_CDEF
extern int
${fn}(  
      const ${f1_ctype} * const in,  
      uint64_t nR,
      void *unused,
      ${f2_ctype} * restrict out
      ) 
;
//STOP_FOR_CDEF

   ]],
   definition = [[

#include "${fn}.h"

static inline void __operation(
  ${f1_ctype} a,
  ${f2_ctype} *ptr_c
  )
{
  ${f2_ctype} c;
  ${c_code_for_operator}  
  *ptr_c = c;
}

int
${fn}(  
      const ${f1_ctype} * const in,
      uint64_t nR,  
      void *unused,
      ${f2_ctype} * restrict out
      )

{
  int status = 0;
  if ( in == NULL ) { go_BYE(-1); }
  if ( nR == 0 ) { go_BYE(-1); }

  int nT = sysconf(_SC_NPROCESSORS_ONLN);
#pragma omp parallel for schedule(static) num_threads(nT)
  for ( uint64_t i = 0; i < nR; i++ ) { 
    ${f1_ctype} inv; 
    ${f2_ctype} outv; 
    inv = in[i];
    __operation(inv, &outv);
    out[i] = outv;
  } 
  BYE:
  return status;
}
   ]]
}
