return require 'Q/UTILS/lua/code_gen' {
   declaration = [[
#include "q_incs.h"
extern int
${fn}(  
      const ${in_ctype} * const in,  
      uint64_t nR,
      void *unused,
      ${out_ctype} * restrict out
      ) 
;

   ]],
   definition = [[

#include "_${fn}.h"

static inline void __operation(
  ${in_ctype} a,
  ${out_ctype} *ptr_c
  )
{
  ${out_ctype} c;
  ${c_code_for_operator}  
  *ptr_c = c;
}

int
${fn}(  
      const ${in_ctype} * const in,
      uint64_t nR,  
      void *unused,
      ${out_ctype} * restrict out
      )

{
  int status = 0;
  if ( in == NULL ) { go_BYE(-1); }
  if ( nR == 0 ) { go_BYE(-1); }

  int nT = sysconf(_SC_NPROCESSORS_ONLN);
  nT = 4; // undo hard coding
#pragma omp parallel for schedule(static) num_threads(nT)
  for ( uint64_t i = 0; i < nR; i++ ) { 
    ${in_ctype} inv; 
    ${out_ctype} outv; 
    inv = in[i];
    __operation(inv, &outv);
    out[i] = outv;
  } 
  BYE:
  return status;
}
   ]]
}
