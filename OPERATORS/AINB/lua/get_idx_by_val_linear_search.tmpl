   definition = [[

#include "${fn}.h"

int
${fn}(  
      const ${in_ctype} * restrict a,  
      uint32_t nA,
      const ${in_ctype} * restrict b,  
      uint32_t nB,
      ${out_ctype} * restrict c,
      bool * restrict nn_c
      )
